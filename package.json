{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "A minimalist latest version react and react-router skeleton, an exploration for personal use",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "browserify src/main.js -t babelify --outfile public/js/bundle.js",
    "build-css": "./bin/build-css",
    "watch-js": "watchify src/main.js -t babelify --outfile public/js/bundle.js",
    "watch-css": "npm run build-css && node-sass --output-style compressed --include-path styles -w styles -o public/css",
    "watch": "npm run watch-js & npm run watch-css",
    "start": "node index-server.js",
    "build": "npm run build-js & npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awebfactory/react-starter.git"
  },
  "keywords": [],
  "author": "Victor Kane",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/awebfactory/react-starter/issues"
  },
  "homepage": "https://github.com/awebfactory/react-starter#readme",
  "dependencies": {
    "axios": "^0.7.0",
    "body-parser": "^1.14.1",
    "bourbon": "^4.2.6",
    "bourbon-neat": "^1.7.2",
    "express": "^4.13.3",
    "history": "^1.12.4",
    "lodash": "^3.10.1",
    "method-override": "^2.3.5",
    "morgan": "^1.6.1",
    "path": "^0.12.7",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-router": "^1.0.0-rc3",
    "serve-static": "^1.10.0"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "node-sass": "^3.3.3"
  }
}
